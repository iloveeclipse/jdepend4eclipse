<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="JDepend4Eclipse"
      label="JDepend4Eclipse"
      version="1.2.4.qualifier"
      provider-name="Andrey Loskutov"
      plugin="de.loskutov.eclipse.jdepend">

   <description url="https://github.com/iloveeclipse/jdepend4eclipse/wiki">
      JDepend4Eclipse plugin for Eclipse is a wrapper for running JDepend
from within Eclipse.
   </description>

